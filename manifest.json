{
  "manifest_version": 3,
  "name": "Search History",
  "version": "1.0",
  "description": "Search browser history and restore closed windows",
  "permissions": [
    "history",
    "activeTab",
    "tabs",
    "windows",
    "sessions"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Search History"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}